<h2 class='inline_block'>Customer Lookup</h2>
<form id='inline_block' action='/customers/new' method='get'>
  <button type='submit'>New Customer?</button>
</form>

<form action='/customers/lookup' method='post'>
    <label for='first_name'>
      First Name
      <input type='text' name='first_name' id='first_name'>
    </label>

    <label for='last_name'>
      Last Name
      <input type='text' name='last_name' id='last_name'>
    </label>

    <label for='phone_number'>
      Phone Number
      <input type='tele' name='phone_number' id='phone_number'>
    </label>

    <label for='email'>
      Email Address
      <input type=email' name='email' id='email'>
    </label>

    <button type='submit'>Search</button>
    <button type='reset'>Reset</button>
</form>
